{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:react/jsx-runtime",
    "plugin:jest/recommended",
    "plugin:jest/style",
    "prettier"
  ],
  "globals": {
    "JSX": true
  },
  "rules": {
    "jest/valid-title": [
      "error",
      {
        "ignoreTypeOfDescribeName": true
      }
    ]
  }
}
