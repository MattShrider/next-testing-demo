{
  "name": "next-testing-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect=9229' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "concurrently -gc 'magenta,blue' npm:lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --fix .",
    "format:prettier": "prettier --write .",
    "test": "concurrently -gc 'green,yellow' npm:test:*",
    "test-ci": "concurrently -gPc 'green,yellow' \"npm:test:* -- {@}\" -- --ci",
    "test:node": "jest -c jest.node.config.js",
    "test:browser": "jest -c jest.browser.config.js"
  },
  "dependencies": {
    "@types/node": "20.2.6",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^8.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^0.0.0-fetch.rc-14",
    "node-mocks-http": "^1.12.2",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0"
  }
}
